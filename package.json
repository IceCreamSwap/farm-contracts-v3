{
  "name": "IceCreamSwap",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "bsc:testnet:timelock": "truffle migrate --f 2 --to 2 --network testnet",
    "bsc:mainnet:timelock": "truffle migrate --f 2 --to 2 --network mainnet",
    "bsc:testnet:farm": "truffle migrate --f 3 --to 3 --network testnet",
    "bsc:mainnet:farm": "truffle migrate --f 3 --to 3 --network mainnet",
    "bsc:testnet:factory": "truffle migrate --f 4 --to 4 --network testnet",
    "bsc:mainnet:factory": "truffle migrate --f 4 --to 4 --network mainnet",
    "bsc:testnet:weth": "truffle migrate --f 5 --to 5 --network testnet ",
    "bsc:mainnet:weth": "truffle migrate --f 5 --to 5 --network mainnet ",
    "bsc:testnet:router": "truffle migrate --f 6 --to 6 --network testnet ",
    "bsc:mainnet:router": "truffle migrate --f 6 --to 6 --network mainnet ",
    "dev:farm": "truffle migrate --f 3 --to 3 --network dev --reset",
    "dev:factory": "truffle migrate --f 4 --to 4 --network dev --reset",
    "dev:weth": "truffle migrate --f 5 --to 5 --network dev --reset",
    "dev:router": "truffle migrate --f 6 --to 6 --network dev --reset",
    "migrate": "truffle migrate --network dev --reset",
    "compile": "truffle compile --network dev",
    "dev:test:farm": "truffle test tests/Farm.test.js --network dev --bail",
    "dev:test:vault": "truffle test tests/Vault.test.js --network dev --bail",
    "test": "truffle compile --all --network dev && nodemon --exec yarn dev:test:farm -e sol,js",
    "test:vault": "truffle compile --all --network dev  && nodemon --exec yarn dev:test:vault -e sol,js",
    "live": "yarn test"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.2.0",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/test-helpers": "^0.5.6",
    "@pancakeswap/pancake-swap-lib": "^0.0.1",
    "@truffle/hdwallet-provider": "^1.4.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.14",
    "numeral": "^2.0.6",
    "solidity-coverage": "^0.7.13",
    "truffle": "^5.1.58",
    "truffle-flattener": "^1.4.4",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-plugin-verify": "^0.5.4",
    "truffle-privatekey-provider": "^1.5.0",
    "web3": "^1.3.0",
    "web3-provider-engine": "^16.0.1"
  }
}
